{
  "base_url":"https:\/\/localhost:8443\/cas",
  "protocol":{
    "login":{
      "path":"\/login",
      "default_parameters":{
        "service":"https:\/\/ecasclient\/"
      },
      "allowed_parameters":[
        "service",
        "renew",
        "gateway"
      ]
    },
    "serviceValidate":{
      "path":"\/serviceValidate",
      "allowed_parameters":[
        "service",
        "ticket",
        "pgtUrl"
      ],
      "default_parameters":{
        "userDetails":"true",
        "format":"XML",
        "pgtUrl":"https:\/\/ecasclient\/includes\/proxyCallback.php"
      }
    },
    "logout":{
      "path":"\/logout",
      "allowed_parameters":[
        "service"
      ],
      "default_parameters":{
        "service":"https:\/\/ecasclient"
      }
    },
    "proxy":{
      "path":"\/proxy",
      "allowed_parameters":[
        "pgt",
        "targetService"
      ]
    },
    "proxyValidate":{
      "path":"\/proxyValidate",
      "allowed_parameters":[
        "ticket",
        "service",
        "pgtUrl"
      ],
      "default_parameters":{
        "userDetails":"true",
        "format":"XML",
        "pgtUrl":"https:\/\/ecasclient\/proxyCallback.php"
      }
    }
  }
}